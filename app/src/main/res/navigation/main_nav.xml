<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.epic.ui.fragment.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_menuFragment"
            app:destination="@id/menuFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_homeFragment_to_allMenuFragment"
            app:destination="@id/allMenuFragment" />
        <action
            android:id="@+id/action_homeFragment_to_listSalesFragment"
            app:destination="@id/listSalesFragment" />
        <action
            android:id="@+id/action_homeFragment_to_listStockFragment"
            app:destination="@id/listStockFragment" />
        <action
            android:id="@+id/action_homeFragment_to_listSellerFragment"
            app:destination="@id/listSellerFragment" />
    </fragment>
    <fragment
        android:id="@+id/menuFragment"
        android:name="com.example.epic.ui.fragment.menu.MenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.epic.ui.fragment.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_imagePickerFragment"
            app:destination="@id/imagePickerFragment" />
        <action
            android:id="@+id/action_profileFragment_to_detailProfileFragment"
            app:destination="@id/detailProfileFragment" />
        <action
            android:id="@+id/action_profileFragment_to_storeAddressFragment"
            app:destination="@id/storeAddressFragment" />
    </fragment>
    <fragment
        android:id="@+id/allMenuFragment"
        android:name="com.example.epic.ui.fragment.menu.AllMenuFragment"
        android:label="fragment_all_menu"
        tools:layout="@layout/fragment_all_menu" >
        <action
            android:id="@+id/action_allMenuFragment_to_listSalesFragment"
            app:destination="@id/listSalesFragment" />
        <action
            android:id="@+id/action_allMenuFragment_to_listCategoryFragment"
            app:destination="@id/listCategoryFragment" />
        <action
            android:id="@+id/action_allMenuFragment_to_listProductFragment"
            app:destination="@id/listProductFragment" />
        <action
            android:id="@+id/action_allMenuFragment_to_listStockFragment"
            app:destination="@id/listStockFragment" />
        <action
            android:id="@+id/action_allMenuFragment_to_listSellerFragment"
            app:destination="@id/listSellerFragment" />
        <action
            android:id="@+id/action_allMenuFragment_to_productReturnFragment"
            app:destination="@id/productReturnFragment" />
        <action
            android:id="@+id/action_allMenuFragment_to_statisticSellerFragment"
            app:destination="@id/statisticSellerFragment" />
        <action
            android:id="@+id/action_allMenuFragment_to_reportFragment"
            app:destination="@id/reportFragment" />
        <action
            android:id="@+id/action_allMenuFragment_to_listStoreFragment"
            app:destination="@id/listStoreFragment" />
        <action
            android:id="@+id/action_allMenuFragment_to_newStatisticFragment"
            app:destination="@id/newStatisticFragment" />
    </fragment>
    <fragment
        android:id="@+id/listSalesFragment"
        android:name="com.example.epic.ui.fragment.sales.ListSalesFragment"
        android:label="fragment_list_sales"
        tools:layout="@layout/fragment_list_sales" >
        <action
            android:id="@+id/action_listSalesFragment_to_addSalesFragment"
            app:destination="@id/addSalesFragment" />
    </fragment>
    <fragment
        android:id="@+id/listCategoryFragment"
        android:name="com.example.epic.ui.fragment.category.ListCategoryFragment"
        android:label="fragment_list_category"
        tools:layout="@layout/fragment_list_category" >
        <action
            android:id="@+id/action_listCategoryFragment_to_addCategoryFragment"
            app:destination="@id/addCategoryFragment" />
        <action
            android:id="@+id/action_listCategoryFragment_to_updateCategoryFragment"
            app:destination="@id/updateCategoryFragment" />
    </fragment>
    <fragment
        android:id="@+id/addCategoryFragment"
        android:name="com.example.epic.ui.fragment.category.AddCategoryFragment"
        android:label="fragment_add_category"
        tools:layout="@layout/fragment_add_category" />
    <fragment
        android:id="@+id/listProductFragment"
        android:name="com.example.epic.ui.fragment.product.ListProductFragment"
        android:label="fragment_list_product"
        tools:layout="@layout/fragment_list_product" >
        <action
            android:id="@+id/action_listProductFragment_to_addProductFragment"
            app:destination="@id/addProductFragment" />
        <action
            android:id="@+id/action_listProductFragment_to_updateProductFragment"
            app:destination="@id/updateProductFragment" />
    </fragment>
    <fragment
        android:id="@+id/addProductFragment"
        android:name="com.example.epic.ui.fragment.product.AddProductFragment"
        android:label="fragment_add_product"
        tools:layout="@layout/fragment_add_product" />
    <fragment
        android:id="@+id/listStockFragment"
        android:name="com.example.epic.ui.fragment.stock.ListStockFragment"
        android:label="fragment_list_stock"
        tools:layout="@layout/fragment_list_stock" >
        <action
            android:id="@+id/action_listStockFragment_to_listCategoryByStockFragment"
            app:destination="@id/listCategoryByStockFragment" />
        <action
            android:id="@+id/action_listStockFragment_to_listWarningRefillFragment"
            app:destination="@id/listWarningRefillFragment" />
    </fragment>
    <fragment
        android:id="@+id/addSalesFragment"
        android:name="com.example.epic.ui.fragment.sales.AddSalesFragment"
        android:label="fragment_add_sales"
        tools:layout="@layout/fragment_add_sales" />
    <fragment
        android:id="@+id/listSellerFragment"
        android:name="com.example.epic.ui.fragment.seller.ListSellerFragment"
        android:label="fragment_list_seller"
        tools:layout="@layout/fragment_list_seller" >
        <action
            android:id="@+id/action_listSellerFragment_to_addSellerFragment"
            app:destination="@id/addSellerFragment" />
    </fragment>
    <fragment
        android:id="@+id/addSellerFragment"
        android:name="com.example.epic.ui.fragment.seller.AddSellerFragment"
        android:label="fragment_add_seller"
        tools:layout="@layout/fragment_add_seller" />
    <fragment
        android:id="@+id/updateCategoryFragment"
        android:name="com.example.epic.ui.fragment.category.UpdateCategoryFragment"
        android:label="fragment_update_category"
        tools:layout="@layout/fragment_update_category" >
        <argument
            android:name="dataCategory"
            app:argType="com.example.epic.data.model.category.read.Data" />
    </fragment>
    <fragment
        android:id="@+id/productReturnFragment"
        android:name="com.example.epic.ui.fragment.product_return.ProductReturnFragment"
        android:label="fragment_product_return"
        tools:layout="@layout/fragment_product_return" >
        <action
            android:id="@+id/action_productReturnFragment_to_addProductReturnFragment"
            app:destination="@id/addProductReturnFragment" />
    </fragment>
    <fragment
        android:id="@+id/addProductReturnFragment"
        android:name="com.example.epic.ui.fragment.product_return.AddProductReturnFragment"
        android:label="fragment_add_product_return"
        tools:layout="@layout/fragment_add_product_return" />
    <dialog
        android:id="@+id/imagePickerFragment"
        android:name="com.example.epic.ui.fragment.profile.ImagePickerFragment"
        android:label="fragment_image_picker"
        tools:layout="@layout/fragment_image_picker" />
    <fragment
        android:id="@+id/statisticSellerFragment"
        android:name="com.example.epic.ui.fragment.statistic.StatisticSellerFragment"
        android:label="fragment_statistic_seller"
        tools:layout="@layout/fragment_statistic_seller" >
        <action
            android:id="@+id/action_statisticSellerFragment_to_newStatisticFragment"
            app:destination="@id/newStatisticFragment" />
    </fragment>
    <fragment
        android:id="@+id/reportFragment"
        android:name="com.example.epic.ui.fragment.report.ReportFragment"
        android:label="fragment_report"
        tools:layout="@layout/fragment_report" />
    <fragment
        android:id="@+id/updateProductFragment"
        android:name="com.example.epic.ui.fragment.product.UpdateProductFragment"
        android:label="fragment_update_product"
        tools:layout="@layout/fragment_update_product" >
        <argument
            android:name="dataProduct"
            app:argType="com.example.epic.data.model.product.read.Data"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/listCategoryByStockFragment"
        android:name="com.example.epic.ui.fragment.stock.ListCategoryByStockFragment"
        android:label="fragment_list_category_by_stock"
        tools:layout="@layout/fragment_list_category_by_stock" >
        <argument
            android:name="dataCategory"
            app:argType="com.example.epic.data.model.category.read.Data" />
        <action
            android:id="@+id/action_listCategoryByStockFragment_to_listWarningRefillFragment"
            app:destination="@id/listWarningRefillFragment" />
    </fragment>
    <fragment
        android:id="@+id/listStoreFragment"
        android:name="com.example.epic.ui.fragment.store_management.ListStoreFragment"
        android:label="fragment_list_store"
        tools:layout="@layout/fragment_list_store" >
        <action
            android:id="@+id/action_listStoreFragment_to_createStoreFragment"
            app:destination="@id/createStoreFragment" />
        <action
            android:id="@+id/action_listStoreFragment_to_updateStoreFragment"
            app:destination="@id/updateStoreFragment" />
    </fragment>
    <fragment
        android:id="@+id/createStoreFragment"
        android:name="com.example.epic.ui.fragment.store_management.CreateStoreFragment"
        android:label="fragment_create_store"
        tools:layout="@layout/fragment_create_store" />
    <fragment
        android:id="@+id/updateStoreFragment"
        android:name="com.example.epic.ui.fragment.store_management.UpdateStoreFragment"
        android:label="fragment_update_store"
        tools:layout="@layout/fragment_update_store" >
        <argument
            android:name="userData"
            app:argType="com.example.epic.data.model.user.management.read.Data" />
    </fragment>
    <fragment
        android:id="@+id/listWarningRefillFragment"
        android:name="com.example.epic.ui.fragment.stock.ListWarningRefillFragment"
        android:label="fragment_list_warning_refill"
        tools:layout="@layout/fragment_list_warning_refill" />
    <fragment
        android:id="@+id/detailProfileFragment"
        android:name="com.example.epic.ui.fragment.profile.DetailProfileFragment"
        android:label="fragment_detail_profile"
        tools:layout="@layout/fragment_detail_profile" >
        <argument
            android:name="detailProfile"
            app:argType="com.example.epic.data.model.user.profile.Data" />
        <action
            android:id="@+id/action_detailProfileFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
        <action
            android:id="@+id/action_detailProfileFragment_to_updateSpinnerFragment"
            app:destination="@id/updateSpinnerFragment" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.example.epic.ui.fragment.profile.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" >
        <argument
            android:name="dataValue"
            app:argType="string" />
        <argument android:name="dataTitle"
            app:argType="string" />
        <argument
            android:name="dataName"
            app:argType="string" />
        <argument
            android:name="userId"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/storeAddressFragment"
        android:name="com.example.epic.ui.fragment.profile.StoreAddressFragment"
        android:label="fragment_store_address"
        tools:layout="@layout/fragment_store_address" />
    <fragment
        android:id="@+id/updateSpinnerFragment"
        android:name="com.example.epic.ui.fragment.profile.UpdateSpinnerFragment"
        android:label="fragment_update_spinner"
        tools:layout="@layout/fragment_update_spinner" >
        <argument
            android:name="dataTitle"
            app:argType="string" />
        <argument
            android:name="dataValue"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/newStatisticFragment"
        android:name="com.example.epic.ui.fragment.statistic.NewStatisticFragment"
        android:label="fragment_new_statistic"
        tools:layout="@layout/fragment_new_statistic" />
</navigation>